
    <%- include partial/header%>
  <h1>Waiter Name: <%=data[0].Waiter.nama%></h1>
  <body class="type-r0">
    <a href="/customer">Back to Customer</a>
    <table class="table table-bordered">
      <tr>
        <th class="text-center">SeatingTable</th>
        <th class="text-center">Orders</th>
        <th class="text-center">Status</th>
        <th class="text-center">Queue No</th>
        <th class="text-center">Set Status</th>
      </tr>
      <%data.forEach(z=>{%>
        <%if(z.status ==0){%>
            <%z.status = 'On Progress'%>
        <%} else{%>
            <%z.status = 'Done'%>
            <%}%>
      <tr>
        <td><%=z.dataValues.SeatingTable.tableName%></td>
        <td><%=z.dataValues.FoodList.food%></td>
        <td><%=z.status%></td>
        <td><%=z.noAntrian%></td>
        <td>
          <a href="/customer/waiter/done/<%=z.id%>">Done</a> ||
          <a href="/customer/waiter/onProgress/<%=z.id%>">On Progress</a>
        </td>
      </tr>
      <%})%>
    </table>
    <form action="">
    <input type="hidden" nama="WaiterId" values="<%=data[0].Waiter.nama%>">
</form>
  </body>
</html>
