      font-weight: 700;
      font-style: italic;
      line-height: 3em;
      padding-top: 13em;
      padding-bottom: 13em;
    }

    .type-r2 {
      font-size: 4.5em;
      text-align: center;
      color: darkgray;
      line-height: 0.8em;
      font-weight: 500;
      position: relative;
      display: inline-block;
      margin-left: 30%;
      padding-top: 1em
  }

    .type-r2::after {
      content: '';
      height: 1px;
      background-color: darkgray;
      width: 2em;
      position: absolute;
      top: 0.5em;
      left: -2.5em;
    }

    .type-r2::before {
      content: '';
      height: 1px;
      background-color: darkgray;
      width: 2em;
      position: absolute;
      top: 0.5em;
      right: -2.5em;
    }

    .type-r3 {
      font-size: 4.5em;
      color: darkgray;
      line-height: 1em;
      font-weight: 500;
      padding-top: -3em;
      /*position: relative;
      display: inline-block;*/
      margin-top: -150px;
    }

    .dropdown {
    position: relative;
    display: inline-block;
    }

   .dropdown-content {
      display: none;
      position: absolute;
      background-color: #f9f9f9;
      min-width: 160px;
      box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
      padding: 1px 20px;
      z-index: 1;
  }

    .dropdown:hover .dropdown-content {
      display: block;
    }

    body {
      margin-top: 30px;
      background-color: #FFF5EE;;
      /*#ffffcc*/
      #FFF5EE
