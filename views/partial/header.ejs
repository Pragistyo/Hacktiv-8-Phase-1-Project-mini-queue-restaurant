  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,700" rel="stylesheet">
  <style media="screen">
    *{
      max-width: 100%;
    }
    .type {
      
      color:  darkgrey;
      text-align: center;
      /* background-color: #f6f6f6; */
      font-family: 'Open Sans', sans-serif;
      font-weight: 700;
      font-style: italic;
      line-height: 3em;
      padding-top: 13em;
      padding-bottom: 13em;
    }

    .table-edit{
      color: black;
      font-family: 'Open Sans', sans-serif;
      font-style: italic;
    }
    .type-r0 {
      color: grey;
      text-align: center;
      font-family: 'Open Sans', sans-serif;
      font-weight: 700;
      font-style: italic;
      margin-left: 0%;
      margin-bottom: 0%;
